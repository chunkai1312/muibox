{
  "git": {
    "commitMessage": "chore(release): ${version}",
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "infile": "CHANGELOG.md",
      "preset": "angular",
      "header": "# Changelog"
    }
  }
}
